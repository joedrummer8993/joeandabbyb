@import 'nib'
@import 'jeet'
global-reset()

primary-color = #A0D2A1
t-color-2 = #6EA06F
body
    max-width 100%
    span(1)
header
    background-color primary-color
    span(1)
	
#nav
    width 100%
    color t-color-2
#nav li
    column(1/5 1, gutter: .5)
    margin-top 10px
    display block
    text-align center
banner
	background-color primary-color
	span(1)
.bannerimage
    center(1410px)
    width 100%
footer
    font-size 12px
    center(100%)
    text-align center